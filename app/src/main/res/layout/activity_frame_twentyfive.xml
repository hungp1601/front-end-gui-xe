<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="frameTwentyfiveVM" type="com.hngsapplication.app.modules.frametwentyfive.data.viewmodel.FrameTwentyfiveVM"/>
    </data>
    <LinearLayout android:id="@+id/linearColumnarrowleft"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/_25pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_600">
        <androidx.appcompat.widget.Toolbar android:gravity="start"
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_33pxh"
            android:layout_marginStart="@dimen/_33pxh"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowarrowleft"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageArrowleft"
                    android:layout_height="@dimen/_14pxh"
                    android:layout_marginBottom="@dimen/_9pxv"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:layout_width="@dimen/_14pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowleft"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLichsgiao"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_53pxh"
                    android:layout_weight="0.93"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{frameTwentyfiveVM.frameTwentyfiveModel.txtLichsgiao}"
                    style="@style/txtInterregular25" tools:text="@string/msg_li_ch_s_giao"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearColumn"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_25pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylegray_600">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnrectanglethirtyeight"
                android:layout_height="match_parent"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylewhite_A702cornerRadius">
                <FrameLayout android:id="@+id/frameStackmataikhoan"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_162pxv"
                    android:layout_marginEnd="@dimen/_22pxh"
                    android:layout_marginStart="@dimen/_22pxh"
                    android:layout_marginTop="@dimen/_25pxv" android:layout_width="@dimen/_350pxh">
                    <FrameLayout
                        android:id="@+id/frameStackrectanglethirtyeight"
                        android:layout_gravity="top|start"
                        android:layout_height="@dimen/_87pxv"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginEnd="@dimen/_10pxh" android:layout_width="@dimen/_124pxh">
                        <ImageView
                            android:id="@+id/imageRectangleThirtyEight"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_87pxh"
                            android:layout_width="@dimen/_124pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_rectangle38"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle38"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtTracu"
                            android:layout_gravity="top|start"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_10pxv"
                            android:layout_marginEnd="@dimen/_25pxh"
                            android:layout_marginStart="@dimen/_25pxh"
                            android:layout_marginTop="@dimen/_5pxv"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{frameTwentyfiveVM.frameTwentyfiveModel.txtTracu}"
                            style="@style/txtIntermedium16_1" tools:text="@string/lbl_tra_c_u"/>
                    </FrameLayout>
                    <LinearLayout
                        android:id="@+id/linearColumnmataikhoan"
                        android:layout_gravity="bottom|start"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="match_parent"
                        android:orientation="vertical" style="@style/groupStylebluegray_2">
                        <LinearLayout android:gravity="start"
                            android:id="@+id/linearRowmataikhoan"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_16pxh"
                            android:layout_marginStart="@dimen/_16pxh"
                            android:layout_marginTop="@dimen/_11pxv"
                            android:layout_width="match_parent" android:orientation="horizontal">
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtMataikhoan"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_2pxv"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_weight="0.38"
                                android:layout_width="0dp"
                                android:singleLine="true"
                                android:text="@{frameTwentyfiveVM.frameTwentyfiveModel.txtMataikhoan}"
                                style="@style/txtIntermedium18" tools:text="@string/lbl_ma_tai_khoan2"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtGroupSeventyFive"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_23pxh"
                                android:layout_weight="0.62"
                                android:layout_width="0dp"
                                android:paddingBottom="@dimen/_2pxv"
                                android:paddingStart="@dimen/_30pxh"
                                android:paddingTop="@dimen/_2pxv"
                                android:singleLine="true"
                                android:text="@{frameTwentyfiveVM.frameTwentyfiveModel.txtGroupSeventyFive}"
                                style="@style/txtSolidRounded_1" tools:text="@string/lbl_pd00001113"/>
                        </LinearLayout>
                        <LinearLayout android:id="@+id/linearRowtngay"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_16pxh"
                            android:layout_marginStart="@dimen/_16pxh"
                            android:layout_marginTop="@dimen/_13pxv"
                            android:layout_width="match_parent" android:orientation="horizontal">
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtTngay"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_weight="0.28"
                                android:layout_width="0dp"
                                android:singleLine="true"
                                android:text="@{frameTwentyfiveVM.frameTwentyfiveModel.txtTngay}"
                                style="@style/txtIntermedium18" tools:text="@string/lbl_t_nga_y"/>
                            <View
                                android:background="@drawable/rectangle_bg_white_a701_radius_5"
                                android:id="@+id/viewRectangleFortyFive"
                                android:layout_height="@dimen/_23pxv"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_marginStart="@dimen/_66pxh" android:layout_width="@dimen/_179pxh"/>
                        </LinearLayout>
                        <LinearLayout android:id="@+id/linearRownngay"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_18pxv"
                            android:layout_marginEnd="@dimen/_16pxh"
                            android:layout_marginStart="@dimen/_16pxh"
                            android:layout_marginTop="@dimen/_12pxv"
                            android:layout_width="match_parent" android:orientation="horizontal">
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtNngay"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_3pxv"
                                android:layout_weight="0.32"
                                android:layout_width="0dp"
                                android:singleLine="true"
                                android:text="@{frameTwentyfiveVM.frameTwentyfiveModel.txtNngay}"
                                style="@style/txtIntermedium18" tools:text="@string/lbl_n_nga_y"/>
                            <View
                                android:background="@drawable/rectangle_bg_white_a701_radius_5"
                                android:id="@+id/viewRectangleFortySix"
                                android:layout_height="@dimen/_25pxv"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_marginStart="@dimen/_51pxh" android:layout_width="@dimen/_178pxh"/>
                        </LinearLayout>
                    </LinearLayout>
                </FrameLayout>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnTimKim"
                    android:layout_gravity="end"
                    android:layout_height="@dimen/_34pxv"
                    android:layout_marginEnd="@dimen/_23pxh"
                    android:layout_marginStart="@dimen/_23pxh"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_width="@dimen/_134pxh"
                    android:paddingBottom="@dimen/_6pxv"
                    android:paddingEnd="@dimen/_29pxh"
                    android:paddingStart="@dimen/_28pxh"
                    android:paddingTop="@dimen/_6pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_ti_m_ki_m"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded_2" tools:text="@string/lbl_ti_m_ki_m"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtGroupSeventySix"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_22pxh"
                    android:layout_marginStart="@dimen/_22pxh"
                    android:layout_marginTop="@dimen/_15pxv"
                    android:layout_width="wrap_content"
                    android:paddingBottom="@dimen/_2pxv"
                    android:paddingStart="@dimen/_27pxh"
                    android:paddingTop="@dimen/_10pxv"
                    android:singleLine="true"
                    android:text="@{frameTwentyfiveVM.frameTwentyfiveModel.txtGroupSeventySix}"
                    style="@style/txtSolidRoundedTopCorner" tools:text="@string/msg_k_t_qua_tra_c"/>
                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/recyclerTable"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_459pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_width="match_parent"
                    android:orientation="vertical"
                    app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                    itemSpace="@{@dimen/_1pxh}"
                    style="@style/groupStylewhite_5" tools:itemCount="2" tools:listitem="@layout/row_table1"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
