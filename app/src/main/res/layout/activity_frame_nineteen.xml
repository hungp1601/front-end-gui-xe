<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="frameNineteenVM" type="com.hngsapplication.app.modules.framenineteen.data.viewmodel.FrameNineteenVM"/>
    </data>
    <LinearLayout android:id="@+id/linearColumnarrowleft"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/_28pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_600">
        <androidx.appcompat.widget.Toolbar android:gravity="start"
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_33pxh"
            android:layout_marginStart="@dimen/_33pxh"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:id="@+id/linearRowarrowleft"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageArrowleft"
                    android:layout_height="@dimen/_14pxh"
                    android:layout_marginBottom="@dimen/_12pxv"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_width="@dimen/_14pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowleft"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLichs"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_113pxh"
                    android:layout_weight="0.86"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{frameNineteenVM.frameNineteenModel.txtLichs}"
                    style="@style/txtInterregular25" tools:text="@string/lbl_li_ch_s"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearColumn"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_22pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylegray_600">
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearColumnlichsxeva"
                android:layout_height="match_parent"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylewhite_A702cornerRadius">
                <LinearLayout android:id="@+id/linearRowlichsxeva"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_32pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtLichsxeva"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_6pxv"
                        android:layout_weight="0.91"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{frameNineteenVM.frameNineteenModel.txtLichsxeva}"
                        style="@style/txtInterregular18" tools:text="@string/msg_li_ch_s_xe_va"/>
                    <ImageButton android:background="@color/white_A701"
                        android:id="@+id/btnVector"
                        android:layout_height="@dimen/_24pxh"
                        android:layout_marginStart="@dimen/_83pxh"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_width="@dimen/_24pxh"
                        android:paddingHorizontal="@dimen/_7pxh"
                        android:paddingVertical="@dimen/_7pxv"
                        android:scaleType="fitCenter"
                        android:src="@drawable/img_vector_bluegray_900" tools:ignore="ContentDescription"/>
                </LinearLayout>
                <View android:background="@color/gray_700"
                    android:id="@+id/lineLineTwo"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginEnd="@dimen/_9pxh"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_25pxv" android:layout_width="@dimen/_380pxh"/>
                <LinearLayout android:id="@+id/linearRowlichsgiao"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_29pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtLichsgiao"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_6pxv"
                        android:layout_weight="0.86"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{frameNineteenVM.frameNineteenModel.txtLichsgiao}"
                        style="@style/txtInterregular18" tools:text="@string/msg_li_ch_s_giao"/>
                    <ImageButton android:background="@color/white_A701"
                        android:id="@+id/btnVectorOne"
                        android:layout_height="@dimen/_24pxh"
                        android:layout_marginStart="@dimen/_175pxh"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_width="@dimen/_24pxh"
                        android:paddingHorizontal="@dimen/_7pxh"
                        android:paddingVertical="@dimen/_7pxv"
                        android:scaleType="fitCenter"
                        android:src="@drawable/img_vector_bluegray_900" tools:ignore="ContentDescription"/>
                </LinearLayout>
                <View android:background="@color/gray_700"
                    android:id="@+id/lineLineTwoOne"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginBottom="@dimen/_20pxv"
                    android:layout_marginEnd="@dimen/_9pxh"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_27pxv" android:layout_width="@dimen/_380pxh"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
