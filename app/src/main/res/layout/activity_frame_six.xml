<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="frameSixVM" type="com.hngsapplication.app.modules.framesix.data.viewmodel.FrameSixVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearColumnarrowleft"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/_33pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_600">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_29pxh"
            android:layout_marginStart="@dimen/_32pxh"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:id="@+id/linearRowarrowleft"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageArrowleft"
                    android:layout_height="@dimen/_14pxh"
                    android:layout_marginBottom="@dimen/_3pxv"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_width="@dimen/_14pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowleft"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                <ImageView android:id="@+id/imageVector"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_marginStart="@dimen/_303pxh"
                    android:layout_width="@dimen/_20pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_vector"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_vector"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <ScrollView android:clipToPadding="false"
            android:fillViewport="true" android:gravity="start"
            android:id="@+id/scrollViewScrollview"
            android:layout_height="0dp"
            android:layout_marginTop="@dimen/_5pxv"
            android:layout_weight="1" android:layout_width="match_parent">
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearColumnthngtinxe"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtThngtinxe"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_111pxh"
                    android:layout_marginStart="@dimen/_111pxh"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{frameSixVM.frameSixModel.txtThngtinxe}"
                    style="@style/txtInterregular28" tools:text="@string/lbl_th_ng_tin_xe"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearColumngroup"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_34pxv"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStylewhite_6">
                    <ImageView android:id="@+id/imageGroup"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="@dimen/_88pxh"
                        android:layout_marginEnd="@dimen/_65pxh"
                        android:layout_marginStart="@dimen/_65pxh"
                        android:layout_marginTop="@dimen/_76pxv"
                        android:layout_width="@dimen/_153pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_group"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_group"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtBinsxeThree"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_65pxh"
                        android:layout_marginStart="@dimen/_65pxh"
                        android:layout_marginTop="@dimen/_47pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{frameSixVM.frameSixModel.txtBinsxeThree}"
                        style="@style/txtInterregular20_1" tools:text="@string/msg_bi_n_s_xe_3"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtMaubinBi"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_65pxh"
                        android:layout_marginStart="@dimen/_65pxh"
                        android:layout_marginTop="@dimen/_14pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{frameSixVM.frameSixModel.txtMaubinBi}"
                        style="@style/txtInterregular20_1" tools:text="@string/msg_ma_u_bi_n_bi"/>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearRowtrangthai"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_448pxv"
                        android:layout_marginEnd="@dimen/_65pxh"
                        android:layout_marginStart="@dimen/_65pxh"
                        android:layout_marginTop="@dimen/_6pxv"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtTrangthai"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_5pxv"
                            android:layout_weight="0.49"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{frameSixVM.frameSixModel.txtTrangthai}"
                            style="@style/txtInterregular20_1" tools:text="@string/lbl_tra_ng_tha_i2"/>
                        <androidx.appcompat.widget.AppCompatButton
                            android:ellipsize="end"
                            android:gravity="center"
                            android:id="@+id/btnHoatng"
                            android:layout_height="@dimen/_29pxv"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginStart="@dimen/_11pxh"
                            android:layout_width="@dimen/_104pxh"
                            android:paddingBottom="@dimen/_6pxv"
                            android:paddingEnd="@dimen/_17pxh"
                            android:paddingStart="@dimen/_17pxh"
                            android:paddingTop="@dimen/_6pxv"
                            android:singleLine="true"
                            android:text="@string/lbl_hoa_t_ng"
                            android:textAllCaps="false"
                            style="@style/btnSolidRounded_3" tools:text="@string/lbl_hoa_t_ng"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</layout>
